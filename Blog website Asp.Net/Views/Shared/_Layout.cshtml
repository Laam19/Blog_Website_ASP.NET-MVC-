<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width", initial-scale="1.0" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <div>
        <a asp-controller="Home" asp-action="index">Home</a>
        <a asp-controller="Home" asp-action="Edit">createpost</a>
        <a asp-controller="Panel" asp-action="index">adminPanel</a>
        @if (User.Identity.IsAuthenticated)
        {
            <a asp-controller="Auth" asp-action="Logout">SignOut</a>
        }
        else
        {

            <a asp-controller="Auth" asp-action="Login">Sign In</a>
            
        }

    </div>
    <div>
        @RenderBody()
    </div>
</body>
</html>
